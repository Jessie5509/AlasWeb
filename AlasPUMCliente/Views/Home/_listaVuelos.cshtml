@model List<Common.DTO.DtoVuelo>


@foreach (Common.DTO.DtoVuelo item in Model)
{


    <div class="card">
        <div id="card" class="panel panel-default" style="width: 250px; height: 250px; float:left;margin-left:10px; margin-bottom:150px; margin-right:15px;margin-top:15px; background-color: rgb(255, 255, 255)">

            <div id="portada" class="card-title" style="margin-left: 8px; font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif; font-size: large;">@Html.DisplayFor(modelItem => item.numeroVuelo)</div>

            <div class="panel-body" style="width: 200px; height: 200px;">
              @*imagen*@  <img class="img-responsive" src="@Html.DisplayFor(modelItem => item.)" style="width:auto; height: 175px;">

            </div>
            <div class="panel-footer">
                <p>$ @Html.DisplayFor(modelItem => item.precio)</p>
                <p id="contenido">@Html.DisplayFor(modelItem => item.origen)</p>


                @Html.ActionLink("Ver", "VueloInfo", "Home", new { id = item.numeroVuelo }, new { @class = "btn btn-info" })
            </div>


        </div>
    </div>

}
